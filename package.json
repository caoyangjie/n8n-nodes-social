{
  "name": "n8n-nodes-social",
  "version": "1.0.0",
  "description": "n8n node for Social integration",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "caocao",
    "email": "kingworld890719@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/caoyangjie/n8n-node-social.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/WechatApi.credentials.js",
      "dist/credentials/FeishuApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Wechat/WechatToken.node.js",
      "dist/nodes/Wechat/WechatDraft.node.js",
      "dist/nodes/Wechat/WechatDraftGet.node.js",
      "dist/nodes/Wechat/WechatDraftDelete.node.js",
      "dist/nodes/Wechat/WechatDraftUpdate.node.js",
      "dist/nodes/Wechat/WechatDraftList.node.js",
      "dist/nodes/Wechat/WechatDraftPublish.node.js",
      "dist/nodes/Wechat/WechatMedia.node.js",
      "dist/nodes/Wechat/WechatMaterial.node.js",
      "dist/nodes/Wechat/WechatMaterialUpload.node.js",
      "dist/nodes/Wechat/WechatMaterialGet.node.js",
      "dist/nodes/Wechat/WechatMaterialDelete.node.js",
      "dist/nodes/Wechat/WechatMaterialCount.node.js",
      "dist/nodes/Wechat/WechatMaterialList.node.js",
      "dist/nodes/Wechat/WechatArticleGet.node.js",
      "dist/nodes/Wechat/WechatPublishedArticles.node.js",
      "dist/nodes/Feishu/FeishuToken.node.js",
      "dist/nodes/Feishu/FeishuBitable.node.js",
      "dist/nodes/Feishu/FeishuRootFolder.node.js",
      "dist/nodes/Feishu/FeishuFileList.node.js",
      "dist/nodes/Feishu/FeishuFolderMeta.node.js",
      "dist/nodes/Feishu/FeishuCreateFolder.node.js",
      "dist/nodes/Feishu/FeishuMoveFile.node.js",
      "dist/nodes/Feishu/FeishuDeleteFile.node.js",
      "dist/nodes/Feishu/FeishuTaskCheck.node.js",
      "dist/nodes/Feishu/FeishuBitableCopy.node.js",
      "dist/nodes/Feishu/FeishuBitableGet.node.js",
      "dist/nodes/Feishu/FeishuBitableUpdate.node.js"
    ]
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^1.6.7",
    "form-data": "^4.0.0",
    "n8n-core": "^1.82.0",
    "n8n-workflow": "^1.82.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "npx --package=typescript@5.9.3 tsc && mkdir -p dist/icon && cp src/icon/*.svg dist/icon/",
    "dev": "npx --package=typescript@5.9.3 tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint src",
    "lintfix": "eslint src --fix",
    "prepublishOnly": "npm run build"
  }
}
